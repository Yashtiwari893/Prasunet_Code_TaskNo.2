<RelativeLayout android:id="@+id/RelativeLayout01" android:layout_width="fill_parent" android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TextView_Name" android:text="Name:"></TextView><EditText android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/EditText_Name" android:layout_below="@+id/TextView_Name"></EditText><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TextView_Email" android:layout_below="@+id/EditText_Name" android:text="E-Mail: *"></TextView><EditText android:layout_height="wrap_content" android:id="@+id/EditText_Email" android:layout_width="fill_parent" android:layout_below="@+id/TextView_Email"></EditText><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TextView_Area" android:layout_below="@+id/EditText_Email" android:text="Area: **"></TextView><Spinner android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/Spinner_Area" android:layout_below="@+id/TextView_Area"></Spinner><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TextView_Problem" android:layout_below="@+id/Spinner_Area" android:text="Problem: **"></TextView><EditText android:layout_height="wrap_content" android:scrollbars="vertical" android:lines="5" android:id="@+id/EditText_Problem" android:layout_below="@+id/TextView_Problem" android:layout_width="fill_parent" android:gravity="top"></EditText><TextView android:layout_below="@+id/EditText_Problem" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="* Required if feedback is desired" android:id="@+id/TextView_Instruct1"></TextView>
<TextView android:layout_below="@+id/TextView_Instruct1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TextView_Instruct2" android:text="** Required Fields"></TextView>
<Button android:layout_below="@+id/TextView_Instruct2" android:layout_height="wrap_content" android:text="Submit" android:id="@+id/Button_Submit" android:layout_centerHorizontal="true" android:layout_width="fill_parent"></Button>
</RelativeLayout>
